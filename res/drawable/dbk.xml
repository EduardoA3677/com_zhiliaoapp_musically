<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns="">
    <item n1:id="@android:id/background"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape>
            <corners n1:radius="1.5dp" />
            <solid n1:color="#ffffff" />
        </shape>
    </item>
    <item n2:id="@android:id/progress"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip>
            <shape>
                <corners n2:radius="1.5dp" />
                <solid n2:color="#2486ff" />
            </shape>
        </clip>
    </item>
</layer-list>
