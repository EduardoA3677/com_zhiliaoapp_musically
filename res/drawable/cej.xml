<?xml version="1.0" encoding="utf-8"?>
<selector xmlns="">
    <item n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <set>
            <objectAnimator n1:duration="150" n1:valueTo="0.9" n1:valueType="floatType" n1:propertyName="scaleX" />
            <objectAnimator n1:duration="150" n1:valueTo="0.9" n1:valueType="floatType" n1:propertyName="scaleY" />
        </set>
    </item>
    <item n2:state_pressed="false"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <set>
            <objectAnimator n2:duration="150" n2:valueTo="1" n2:valueType="floatType" n2:propertyName="scaleX" />
            <objectAnimator n2:duration="150" n2:valueTo="1" n2:valueType="floatType" n2:propertyName="scaleY" />
        </set>
    </item>
</selector>
